    "url": "git+https://github.com/wilkerHop/mussum-ipsum-api.git"
  },
  "author": "Wilker Ribeiro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wilkerHop/mussum-ipsum-api/issues"
  },
  "homepage": "https://github.com/wilkerHop/mussum-ipsum-api#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
