    "@typescript-eslint/parser": "^2.6.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^0.14.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.3"
